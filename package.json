{
  "name": "vuejs-masterclass-2024",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "dotenv -e .env supabase link --project-ref $SUPABASE_PROJECT_ID",
    "supabase:type": "dotenv -e .env supabase gen types --lang=typescript --project-id $SUPABASE_PROJECT_ID --schema public > database/types.ts",
    "db:make-migration": "supabase migration new",
    "db:migrate": "supabase migration up --linked",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "db:refresh": "npm run db:reset && npm run db:seed"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/vue-table": "^8.20.5",
    "@vueuse/core": "^11.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.454.0",
    "pinia": "^2.2.4",
    "radix-vue": "^1.9.8",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.29.0",
    "iconify-icon": "^2.1.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "supabase": "^1.207.9",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.0",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.6"
  },
  "volta": {
    "node": "20.18.0"
  }
}
